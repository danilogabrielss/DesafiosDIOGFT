package DesafioBanco;

public class Cliente {
	private int cpf;
	private String nome;	
	
	public Cliente (double cpf,String nome) {
		this.cpf = cpf;
		this.nome = nome;
	}

	public double getCpf() {
		return cpf;
	}

	public String getNome() {
		return nome;
	}
	public String toString() {
		return "Titular: " + this.nome + ", CPF: " + this.cpf;
	}	
}
